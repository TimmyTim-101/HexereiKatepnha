<Application x:Class="HexereiKatepnha.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HexereiKatepnha"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="MyFont">/HexereiKatepnha;Component/Resources/Font/#HYWenHei 85W</FontFamily>
        <ControlTemplate x:Key="ListBoxNoBorder" TargetType="{x:Type ListBox}">
            <ItemsPresenter SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
        </ControlTemplate>
    </Application.Resources>
</Application>